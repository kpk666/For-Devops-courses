{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "PermissionsRules",
            "Effect": "Allow",
            "Principal": "*",
            "Action": "s3:ListBucket",
            "Resource": "arn:aws:s3:::astrovskiy-bucket"
        },
        {
            "Effect": "Allow",
            "Principal": "*",
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:DeleteObject"
            ],
            "Resource": "arn:aws:s3:::astrovskiy-bucket/*"
        }
    ]
}